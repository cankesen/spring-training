{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Person",
  "description": "A Person",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "fullName": {
      "type": "string"
    },
    "gender": {
      "type": "string",
      "enum": ["MALE", "FEMALE", "UNKNOWN"]
      }
    },
    "birthday": {
      "type": "string"
    },
    "age": {
      "type": "number",
      "minimum": 0
    }
  },
  "required": ["fullName", "gender", "age", "title"]
}
