{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Person",
  "description": "A Person",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "fullName": {
      "type": "string"
    },
    "gender": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["MALE", "FEMALE"]
      }
    },
    "birthday": {
      "type": "string"
    },
    "age": {
      "type": "number",
      "minimum": 0,
    }
  },
  "required": ["title", "fullName", "gender"]
}
